{
  "manifest_version": 3,
  "name": "demo-9",
  "version": "1.0.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions":[ 
		"storage"
	],
  "action": {
		"default_title": "storage",
		"default_popup": "popup.html"
	}
}